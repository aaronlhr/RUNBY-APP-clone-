# Edit your .env.local file
cat > .env.local << 'EOF'
# RUNBY MVP - Development Environment
NEXT_PUBLIC_APP_URL=http://localhost:4000
NODE_ENV=development

# Demo keys for development (replace with real ones later)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_demo_key_replace_me
CLERK_SECRET_KEY=sk_test_demo_key_replace_me

NEXT_PUBLIC_SUPABASE_URL=https://demo-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=demo_anon_key_replace_me

NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_demo_stripe_key
STRIPE_SECRET_KEY=sk_test_demo_stripe_key

STRAVA_CLIENT_ID=demo_strava_id
STRAVA_CLIENT_SECRET=demo_strava_secret
STRAVA_REDIRECT_URI=http://localhost:4000/api/auth/strava/callback

NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.demo_mapbox_token
EOF

# Install any missing dependencies
npm install

# Start the app
npm run dev






